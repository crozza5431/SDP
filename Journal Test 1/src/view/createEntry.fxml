<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox id="AnchorPane" alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.CreateEntryController">
  <Label id="title" text="Create Entry" />
  <HBox alignment="CENTER">
    <Label text="Name of Entry: " />
    <TextField fx:id="entryName" />
  </HBox>
  <HBox alignment="CENTER">
    <Label text="Contents: " />
    <TextArea fx:id="entryContents" />
  </HBox>
  <Button defaultButton="true" onAction="#handleCreateEntry" text="Create" />
  <Hyperlink fx:id="Back" onAction="#processBack" text="Go Back" textFill="RED">
    <font>
      <Font name="System Bold" size="12.0" />
    </font>
  </Hyperlink>
  <stylesheets>
    <URL value="@Login.css" />
  </stylesheets>
</VBox>
